<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.jt.bean.gateway.JobInf" table="jobinf">
		<id name="jobId" type="java.lang.Long">
			<column name="JOBID" />
			<generator class="native" />
		</id>
		<property name="jobName" type="java.lang.String">
			<column name="JOBNAME" length="64" not-null="true" />
		</property>
		<property name="jobGroup" type="java.lang.String">
			<column name="JOBGROUP" length="64" not-null="true" />
		</property>
		<property name="jobStatus" type="java.lang.Integer">
			<column name="JOBSTATUS" not-null="true" />
		</property>
		<property name="cronExpression" type="java.lang.String">
			<column name="CRONEXPRESSION" length="225" not-null="true" />
		</property>
		<property name="beanClass" type="java.lang.String">
			<column name="BEANCLASS" length="125" not-null="true" />
		</property>
		<property name="description" type="java.lang.String">
			<column name="DESCRIPTION" length="65535" />
		</property>
		<property name="triggerName" type="java.lang.String">
			<column name="TRIGGERNAME" length="64" not-null="true" />
		</property>
		<property name="triggerGroupName" type="java.lang.String">
			<column name="TRIGGERGROUPNAME" length="64" not-null="true" />
		</property>
		<property name="createTime" type="java.sql.Timestamp" >
			<column name="createtime" length="19" not-null="false" />
		</property>
		<property name="updateTime" type="java.sql.Timestamp">
			<column name="updateTime" length="19" not-null="false" />
		</property>
		
		<property name="indexPath" type="java.lang.String" >
			<column name="INDEXPATH" length="200" not-null="true" />
		</property>
		<property name="sqlIp" type="java.lang.String" >
			<column name="SQLIP" length="50" not-null="true" />
		</property>
		<property name="sqlUser" type="java.lang.String" >
			<column name="SQLUSER" length="50" not-null="true" />
		</property>
		<property name="sqlPw" type="java.lang.String" >
			<column name="SQLPW" length="50" not-null="true" />
		</property>
		<property name="sqlDb" type="java.lang.String" >
			<column name="SQLDB" length="50" not-null="true" />
		</property>
		<property name="sqlPort" type="java.lang.Integer" >
			<column name="SQLPORT" length="11" not-null="true" />
		</property>
		<property name="sqlTable" type="java.lang.String" >
			<column name="SQLTABLE" length="50" not-null="true" />
		</property>
		
		
	</class>
</hibernate-mapping>
