<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.jt.bean.gateway.JobLog" table="joblog">
		<id name="jobId" type="java.lang.Long">
			<column name="JOBID"  not-null="true"/>
		</id>
		<property name="status" type="java.lang.Integer">
			<column name="STATUS" not-null="true" />
		</property>
		<property name="indexSize" type="java.lang.Integer">
			<column name="INDEXSIZE"  not-null="false" />
		</property>
		<property name="exeTime" type="java.lang.Long" >
			<column name="EXETIME" length="19" not-null="false" />
		</property>
		<property name="start" type="java.sql.Timestamp" >
			<column name="START" length="19" not-null="false" />
		</property>
		<property name="end" type="java.sql.Timestamp" >
			<column name="END" length="19" not-null="false" />
		</property>
		
		<property name="indexPath" type="java.lang.String" >
			<column name="INDEXPATH" length="200" not-null="true" />
		</property>
		<property name="sqlIp" type="java.lang.String" >
			<column name="SQLIP" length="50" not-null="true" />
		</property>
		<property name="sqlUser" type="java.lang.String" >
			<column name="SQLUSER" length="50" not-null="true" />
		</property>
		<property name="sqlPw" type="java.lang.String" >
			<column name="SQLPW" length="50" not-null="true" />
		</property>
		<property name="sqlDb" type="java.lang.String" >
			<column name="SQLDB" length="50" not-null="true" />
		</property>
		<property name="sqlPort" type="java.lang.Integer" >
			<column name="SQLPORT" length="11" not-null="true" />
		</property>
		<property name="sqlTable" type="java.lang.String" >
			<column name="SQLTABLE" length="50" not-null="true" />
		</property>
		
	</class>
</hibernate-mapping>
