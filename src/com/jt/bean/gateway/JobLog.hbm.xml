<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.jt.bean.gateway.JobLog" table="joblog">
		<id name="jobId" type="java.lang.Long">
			<column name="JOBID"  not-null="true"/>
		</id>
		<property name="status" type="java.lang.Integer">
			<column name="STATUS" not-null="true" />
		</property>
		<property name="indexSize" type="java.lang.Integer">
			<column name="INDEXSIZE" />
		</property>
		<property name="exeTime" type="java.sql.Timestamp" >
			<column name="EXETIME" length="19" not-null="false" />
		</property>
	</class>
</hibernate-mapping>
