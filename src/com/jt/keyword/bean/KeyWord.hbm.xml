<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
 <hibernate-mapping>
 <class name="com.jt.keyword.bean.KeyWord" table="KEYWORD">
  
    <id name="id" type="java.lang.Integer">
            <column name="ID" />
            <generator class="native" />
        </id>
  <many-to-one class="com.jt.keyword.bean.KeyWord"  lazy="proxy" name="parent" column="PID" />
  <list cascade="all-delete-orphan" lazy="true"  name="childrens"  inverse="false" >
   <key column="PID" />
   <index column="IDX"/>
   <one-to-many class="com.jt.keyword.bean.KeyWord"/>
  </list>
  <property  name="wordvalue"  type="java.lang.String"  column="WORDVALUE"/>
  <property  name="extend"  type="java.lang.String"  column="EXTEND"/>
  <property  name="floor"   type="java.lang.Integer"  column="FLOOR" />
 <property  name="idx"  column="IDX" type="java.lang.Integer"/>
 <property name="createTime" type="java.sql.Timestamp">
            <column name="CREATE_TIME" length="19" not-null="true">
                <comment>创建时间</comment>
            </column>
        </property>
        <property name="updateTime" type="java.sql.Timestamp">
            <column name="UPDATE_TIME" length="19" not-null="true">
                <comment>修改时间</comment>
            </column>
    </property>
 </class>
</hibernate-mapping>
