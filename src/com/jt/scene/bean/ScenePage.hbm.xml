<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.jt.scene.bean.ScenePage" table="scenepage">
		<id name="scenePageId" type="java.lang.Integer">
			<column name="SCENEPAGEID" />
			<generator class="native" />
		</id>
		<property name="sceneWordId" type="java.lang.Integer">
			<column name="SCENEWORDID" length="64" not-null="true" />
		</property>
		<property name="pageTitle" type="java.lang.String">
			<column name="PAGETITLE" length="300" not-null="true" />
		</property>
		<property name="pageLink" type="java.lang.String">
			<column name="PAGELINK" length="300" not-null="true" />
		</property>
		<property name="sjfl" type="java.lang.String">
			<column name="SJFL" length="500" not-null="false" />
		</property>
		<property name="createTime" type="java.sql.Timestamp" >
			<column name="CREATETIME" length="19" not-null="false" />
		</property>
		<property name="updateTime" type="java.sql.Timestamp">
			<column name="UPDATETIME" length="19" not-null="false" />
		</property>
	</class>
</hibernate-mapping>
