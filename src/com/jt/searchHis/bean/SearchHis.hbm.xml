<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.jt.searchHis.bean.SearchHis" table="SEARCHHIS">
		<id name="id" type="java.lang.Long">
			<column name="ID"  not-null="true"/>
			<generator class="native" />
		</id>
		<property name="searchContent" type="java.lang.String">
			<column name="SEARCHCONTENT" not-null="true" />
		</property>
		<property name="contentMd5" type="java.lang.String">
			<column name="CONTENTMD5" not-null="true" />
		</property>
		
		<property name="searchTimes" type="java.lang.Integer">
			<column name="SEARCHTIMES"  not-null="false" />
		</property>
		<property name="createTime" type="java.sql.Timestamp" >
			<column name="createtime" length="19" not-null="false" />
		</property>
		<property name="updateTime" type="java.sql.Timestamp">
			<column name="updateTime" length="19" not-null="false" />
		</property>
	</class>
</hibernate-mapping>
